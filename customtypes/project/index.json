{
  "id": "project",
  "label": "Project",
  "format": "page",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "config": {
          "label": "UID"
        },
        "type": "UID"
      },
      "title": {
        "type": "Text",
        "config": {
          "label": "Title",
          "placeholder": ""
        }
      },
      "status": {
        "type": "Select",
        "config": {
          "label": "Status",
          "placeholder": "",
          "options": ["Published", "Archived"],
          "default_value": "Published"
        }
      },
      "excerpt": {
        "type": "StructuredText",
        "config": {
          "label": "Excerpt",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "heading2,strong,em,hyperlink"
        }
      },
      "content": {
        "type": "StructuredText",
        "config": {
          "label": "Content",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "strong,em,hyperlink"
        }
      },
      "main_media": {
        "type": "Link",
        "config": {
          "label": "Main media",
          "placeholder": "",
          "select": "media"
        }
      },
      "creation": {
        "type": "Date",
        "config": {
          "label": "Creation",
          "placeholder": ""
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {}
        }
      }
    }
  }
}
